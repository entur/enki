{
  "name": "enki",
  "version": "0.0.2",
  "author": "Entur",
  "private": true,
  "scripts": {
    "postinstall": "check-node-version --npm ~9.5.1",
    "test": "react-scripts test --env=jsdom --modulePaths=src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "3.8.8",
    "@entur/a11y": "0.2.72",
    "@entur/alert": "0.15.12",
    "@entur/button": "3.2.3",
    "@entur/chip": "0.6.58",
    "@entur/datepicker": "8.1.7",
    "@entur/dropdown": "4.0.14",
    "@entur/expand": "3.4.19",
    "@entur/form": "7.0.30",
    "@entur/grid": "0.3.38",
    "@entur/icons": "6.13.0",
    "@entur/layout": "2.1.51",
    "@entur/menu": "4.2.9",
    "@entur/modal": "1.7.23",
    "@entur/table": "4.6.32",
    "@entur/tokens": "3.11.1",
    "@entur/tooltip": "2.6.39",
    "@entur/typography": "1.8.18",
    "@reduxjs/toolkit": "1.9.7",
    "@sentry/react": "7.88.0",
    "axios": "1.6.2",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "duration-fns": "3.0.2",
    "file-saver": "2.0.5",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "leaflet": "1.9.4",
    "lodash.clonedeep": "4.5.0",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.4",
    "oidc-client-ts": "2.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-intl": "6.5.5",
    "react-leaflet": "4.2.1",
    "react-oidc-context": "2.3.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.21.0",
    "redux-sentry-middleware": "0.2.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@types/file-saver": "2.0.7",
    "@types/jest": "27.5.2",
    "@types/leaflet": "1.9.8",
    "@types/lodash.clonedeep": "4.5.9",
    "@types/lodash.isempty": "4.4.9",
    "@types/lodash.isequal": "4.5.8",
    "@types/node": "18.19.3",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-helmet": "6.1.11",
    "@types/react-intl-redux": "0.1.19",
    "@types/react-leaflet": "2.8.3",
    "@types/react-modal": "3.16.3",
    "@types/redux-immutable-state-invariant": "2.1.4",
    "@types/uuid": "9.0.7",
    "check-node-version": "4.2.1",
    "husky": "8.0.3",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.4",
    "react-scripts": "5.0.1",
    "redux-devtools-extension": "2.13.9",
    "redux-immutable-state-invariant": "2.1.0",
    "sass": "1.69.5",
    "source-map-explorer": "2.5.3",
    "typescript": "5.2.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": "9.9.2",
    "node": "18.19.0"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "5.2.2"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@axios)/"
    ]
  }
