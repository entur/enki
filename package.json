{
  "name": "enki",
  "version": "0.0.2",
  "author": "Entur",
  "private": true,
  "scripts": {
    "postinstall": "check-node-version --npm ~9.5.1",
    "test": "react-scripts test --env=jsdom --modulePaths=src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@apollo/client": "3.7.12",
    "@entur/alert": "0.14.0",
    "@entur/auth-provider": "1.1.0",
    "@entur/button": "3.0.6",
    "@entur/chip": "0.6.33",
    "@entur/datepicker": "8.0.1",
    "@entur/dropdown": "4.0.7",
    "@entur/expand": "3.3.44",
    "@entur/form": "7.0.6",
    "@entur/icons": "6.2.0",
    "@entur/layout": "2.1.30",
    "@entur/menu": "4.1.39",
    "@entur/modal": "1.6.34",
    "@entur/table": "4.6.6",
    "@entur/tokens": "3.8.1",
    "@entur/tooltip": "2.6.16",
    "@entur/typography": "1.7.16",
    "@reduxjs/toolkit": "1.9.5",
    "@sentry/react": "7.49.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/file-saver": "2.0.5",
    "@types/jest": "25.2.3",
    "@types/leaflet": "1.9.3",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/lodash.isempty": "4.4.7",
    "@types/lodash.isequal": "4.5.6",
    "@types/node": "13.13.30",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@types/react-helmet": "6.1.6",
    "@types/react-intl-redux": "0.1.16",
    "@types/react-leaflet": "2.8.3",
    "@types/react-modal": "3.16.0",
    "@types/uuid": "9.0.1",
    "axios": "0.27.2",
    "check-node-version": "4.2.1",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "duration-fns": "3.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "enzyme-to-json": "3.6.2",
    "file-saver": "2.0.5",
    "graphql": "16.6.0",
    "graphql-request": "6.0.0",
    "husky": "3.1.0",
    "jest-sonar-reporter": "2.0.0",
    "leaflet": "1.9.3",
    "lint-staged": "9.5.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.4",
    "node-sass": "8.0.0",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-intl": "6.4.1",
    "react-leaflet": "2.8.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.10.0",
    "react-scripts": "5.0.1",
    "redux-devtools-extension": "2.13.9",
    "redux-sentry-middleware": "0.2.2",
    "source-map-explorer": "2.5.3",
    "typescript": "5.0.4",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/redux-immutable-state-invariant": "2.1.2",
    "prettier-plugin-organize-imports": "3.2.2",
    "redux-immutable-state-invariant": "2.1.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
