{
  "name": "enki",
  "version": "0.0.2",
  "author": "Entur",
  "private": true,
  "scripts": {
    "postinstall": "check-node-version --npm ~9.5.1",
    "test": "react-scripts test --env=jsdom --modulePaths=src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "3.7.13",
    "@entur/a11y": "0.2.65",
    "@entur/alert": "0.14.2",
    "@entur/auth-provider": "1.1.1",
    "@entur/button": "3.0.7",
    "@entur/chip": "0.6.35",
    "@entur/datepicker": "8.0.4",
    "@entur/dropdown": "4.0.9",
    "@entur/expand": "3.4.0",
    "@entur/form": "7.0.8",
    "@entur/icons": "6.3.0",
    "@entur/layout": "2.1.32",
    "@entur/menu": "4.1.41",
    "@entur/modal": "1.7.2",
    "@entur/table": "4.6.8",
    "@entur/tokens": "3.9.0",
    "@entur/tooltip": "2.6.18",
    "@entur/typography": "1.7.17",
    "@reduxjs/toolkit": "1.9.5",
    "@sentry/react": "7.50.0",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "duration-fns": "3.0.2",
    "file-saver": "2.0.5",
    "graphql": "16.6.0",
    "graphql-request": "6.0.0",
    "leaflet": "1.9.3",
    "lodash.clonedeep": "4.5.0",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-intl": "6.4.2",
    "react-leaflet": "4.2.1",
    "react-redux": "8.0.5",
    "react-router-dom": "6.11.0",
    "redux-sentry-middleware": "0.2.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/file-saver": "2.0.5",
    "@types/jest": "27.5.2",
    "@types/leaflet": "1.9.3",
    "@types/lodash.clonedeep": "4.5.7",
    "@types/lodash.isempty": "4.4.7",
    "@types/lodash.isequal": "4.5.6",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/react-helmet": "6.1.6",
    "@types/react-intl-redux": "0.1.16",
    "@types/react-leaflet": "2.8.3",
    "@types/react-modal": "3.16.0",
    "@types/redux-immutable-state-invariant": "2.1.2",
    "@types/uuid": "9.0.1",
    "check-node-version": "4.2.1",
    "husky": "8.0.3",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "react-scripts": "5.0.1",
    "redux-devtools-extension": "2.13.9",
    "redux-immutable-state-invariant": "2.1.0",
    "sass": "1.62.1",
    "source-map-explorer": "2.5.3",
    "typescript": "5.0.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": "^9.5.1",
    "node": "^18.16.0"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@axios)/"
    ]
  }
}
