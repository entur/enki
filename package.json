  "name": "order-transport",
  "version": "0.0.1",
  "author": "Sondre Bjerkerud",
  "private": true,
  "dependencies": {
    "@entur/component-library": "^0.2.13",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.2",
    "graphql-request": "^1.6.0",
    "keycloak-js": "^3.4.3",
    "leaflet": "^1.3.4",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "npm": "^6.4.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "raven-js": "^3.26.3",
    "react": "^16.4.1",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-intl-redux": "^0.7.0",
    "react-leaflet": "^2.0.1",
    "react-modal": "^3.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-table": "^6.8.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-raven-middleware": "^1.2.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "cross-env": "5.1.1",
    "css-loader": "0.28.4",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "4.16.2",
    "express-http-proxy": "^1.4.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "23.2.0",
    "jest-cli": "23.2.0",
    "jsdom": "11.12.0",
    "jsdom-global": "3.0.2",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "2.0.6",
    "regenerator-runtime": "^0.12.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "watch-cli": "^0.2.3",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1"
  },
  "scripts": {
    "start-js": "node scripts/start.js",
    "build-js": "node scripts/build.js",
    "build-messages": "cross-env node scripts/group-messages.js",
    "watch-messages": "watch -p './src/i18n/translations/**.json' -c 'npm run build-messages'",
    "test": "node scripts/test.js --env=node",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npm-run-all -p build-messages watch-messages watch-css start-js",
    "start-devenv": "cross-env npm run start",
    "build": "npm-run-all build-css build-messages build-js",
    "eslint": "eslint src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "testMatch": [
      "**/src/**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "globalSetup": "<rootDir>/scripts/testSetup.js",
    "globalTeardown": "<rootDir>/scripts/testTeardown.js"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-function-bind",
      "transform-class-properties"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          "transform-function-bind",
          "transform-class-properties"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true
  }
}
