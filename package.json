    "file-saver": "2.0.5",
    "graphql": "15.8.0",
    "graphql-request": "1.8.2",
    "helmet": "3.23.3",
    "husky": "3.1.0",
    "jest-sonar-reporter": "2.0.0",
    "leaflet": "1.9.3",
    "lint-staged": "9.5.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.4",
    "node-sass": "4.14.1",
    "prettier": "2.8.7",
